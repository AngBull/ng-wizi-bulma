{
  "name": "ng-wizi-bulma",
  "version": "2.0.3",
  "license": "MIT",
  "scripts": {
    "chokidar": "chokidar",
    "concurrently": "concurrently",
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "concurrently \"chokidar './src/lib/ng-wizi-bulma/**/*' -c 'npm run build:lib'\" \"ng serve\"",
    "build:lib": "ng-packagr -p src/lib/ng-package.json",
    "build:app": "cpr dist/ng-wizi-bulma src/demo/ng-wizi-bulma && ng build --prod && rimraf src/demo/ng-wizi-bulma",
    "build": "npm run build:lib && npm run build:app",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && firebase deploy",
    "publish:npm": "cd dist/ng-wizi-bulma && npm publish --access public && cd ../../",
    "publish": "npm run deploy && npm run publish:npm"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/common": "^5.2.7",
    "@angular/compiler": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@angular/forms": "^5.2.7",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@angular/router": "^5.2.7",
    "bulma": "^0.6.2",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.2",
    "@angular/compiler-cli": "^5.2.7",
    "@angular/language-service": "^5.2.7",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~9.4.6",
    "chokidar-cli": "^1.2.0",
    "codelyzer": "^4.1.0",
    "concurrently": "^3.5.1",
    "cpr": "^3.0.1",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.1.0",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.2",
    "ts-node": "~5.0.0",
    "tsickle": "^0.27.2",
    "tslint": "~5.9.1",
    "typescript": "^2.5.3"
  }
}
