{
  "name": "@wizishop/ng-wizi-bulma",
  "version": "7.0.8",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run watch-scss\" \"ng build ng-wizi-bulma --watch \" \"ng serve\"",
    "build:lib": "ng build ng-wizi-bulma --prod && npm run block-scss",
    "build:demo": "ng build ng-wizi-bulma-demo --prod",
    "build": "rm -rf dist && npm run build:lib && npm run build:demo && cp README.md dist/@wizishop/ng-wizi-bulma",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && firebase deploy",
    "publish:npm": "cd dist/@wizishop/ng-wizi-bulma && npm publish --access public && cd ../../../",
    "publish": "npm run deploy && npm run publish:npm",
    "precommit": "pretty-quick --staged",
    "prepare": "ng build ng-wizi-bulma && npm run block-scss",
    "watch-scss": "nodemon --watch './projects/ng-wizi-bulma/src/**/*.scss' -e scss -d 1 -x \"npm run block-scss\" ",
    "block-scss": "node ./scripts/scss-bundle.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.9",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "7.2.9",
    "@angular/compiler": "7.2.9",
    "@angular/core": "7.2.9",
    "@angular/forms": "7.2.9",
    "@angular/platform-browser": "7.2.9",
    "@angular/platform-browser-dynamic": "7.2.9",
    "@angular/router": "7.2.9",
    "bulma": "^0.7.4",
    "bulma-extensions": "^6.2.3",
    "bundle-scss": "^1.3.4",
    "colors": "^1.3.3",
    "core-js": "^2.5.4",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.6",
    "@angular-devkit/build-ng-packagr": "~0.12.2",
    "@angular-devkit/schematics": "^7.3.6",
    "@angular/cli": "^7.3.6",
    "@angular/compiler-cli": "7.2.9",
    "@angular/language-service": "7.2.9",
    "@types/jasmine": "~2.8.11",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.9",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.0",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.6.0",
    "nodemon": "^1.18.9",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.1",
    "tsickle": "0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.4"
  }
}
