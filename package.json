{
  "name": "@wizishop/ng-wizi-bulma",
  "version": "7.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng build ng-wizi-bulma --watch \" \"ng serve\"",
    "build:lib": "ng build ng-wizi-bulma --prod",
    "build:demo": "ng build ng-wizi-bulma-demo --prod",
    "build": "rm -rf dist && npm run build:lib && npm run build:demo && cp README.md dist/@wizishop/ng-wizi-bulma",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && firebase deploy",
    "publish:npm": "cd dist/@wizishop/ng-wizi-bulma && npm publish --access public && cd ../../../",
    "publish": "npm run deploy && npm run publish:npm",
    "precommit": "pretty-quick --staged"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.0",
    "@angular/cdk": "^7.0.0",
    "@angular/common": "7.0.0",
    "@angular/compiler": "7.0.0",
    "@angular/core": "7.0.0",
    "@angular/forms": "7.0.0",
    "@angular/platform-browser": "7.0.0",
    "@angular/platform-browser-dynamic": "7.0.0",
    "@angular/router": "7.0.0",
    "bulma": "^0.7.2",
    "bulma-switch": "^2.0.0",
    "bulma-tooltip": "^2.0.2",
    "core-js": "^2.5.4",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.1",
    "@angular-devkit/schematics": "^7.0.1",
    "@angular/cli": "^7.0.1",
    "@angular/compiler-cli": "7.0.0",
    "@angular/language-service": "7.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~9.4.6",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.0.1",
    "husky": "^1.1.2",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "^4.3.1",
    "prettier": "1.14.3",
    "pretty-quick": "^1.8.0",
    "protractor": "~5.4.0",
    "ts-node": "~6.0.0",
    "tsickle": ">=0.33.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3"
  }
}
