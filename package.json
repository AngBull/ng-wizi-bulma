  "scripts": {
    "chokidar": "chokidar",
    "concurrently": "concurrently",
    "ng": "ng",
    "start": "npm run build:lib && concurrently \"chokidar './projects/ng-wizi-bulma/src/lib/**/*' -c 'npm run build:lib'\" \"ng serve\"",
    "build:lib": "ng build ng-wizi-bulma",
    "build:demo": "ng build ng-wizi-bulma-demo",
    "build": "npm run build:lib && npm run build:demo",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && firebase deploy",
    "publish:npm": "cd dist/ng-wizi-bulma && npm publish --access public && cd ../../",
    "publish": "npm run deploy && npm run publish:npm"
  },
  "private": true,
