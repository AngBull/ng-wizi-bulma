{
  "name": "ng-wizi-bulma",
  "version": "3.0.3",
  "license": "MIT",
  "scripts": {
    "chokidar": "chokidar",
    "concurrently": "concurrently",
    "ng": "ng",
    "start": "ng serve --aot",
    "start:dev": "concurrently \"chokidar './src/lib/ng-wizi-bulma/**/*' -c 'npm run build:lib'\" \"ng serve\"",
    "build:lib": "ng-packagr -p src/lib/ng-package.json",
    "build:app": "cp -r dist/ng-wizi-bulma src/demo/ng-wizi-bulma && ng build --prod && rimraf src/demo/ng-wizi-bulma",
    "build": "npm run build:lib && npm run build:app",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && firebase deploy",
    "publish:npm": "cd dist/ng-wizi-bulma && npm publish --access public && cd ../../",
    "publish": "npm run deploy && npm run publish:npm"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.3",
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/core": "6.0.3",
    "@angular/forms": "6.0.3",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@angular/router": "6.0.3",
    "bulma": "^0.7.1",
    "bulma-switch": "^1.0.2",
    "bulma-tooltip": "^1.0.5",
    "core-js": "^2.5.3",
    "rxjs": "^6.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular-devkit/schematics": "^0.6.0",
    "@angular/cli": "6.0.3",
    "@angular/compiler-cli": "6.0.3",
    "@angular/language-service": "6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~9.4.6",
    "chokidar-cli": "^1.2.0",
    "codelyzer": "^4.1.0",
    "concurrently": "^3.5.1",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "3.0.0-rc.5",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.1",
    "ts-node": "~6.0.0",
    "tsickle": "^0.28.0",
    "tslib": "^1.7.1",
    "tslint": "~5.10.0",
    "typescript": "~2.7.2"
  }
}
